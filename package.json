{
  "name": "loose-memoize",
  "version": "0.1.0",
  "description": "Memoize utility with high performance and loose parameter checks",
  "main": "dist/loose-memoize.js",
  "module": "dist/loose-memoize.esm.js",
  "license": "MIT",
  "author": "Matti Mehtonen <matti@vanils.fi>",
  "homepage": "https://github.com/vanils/loose-memoize",
  "repository": {
    "type": "git",
    "url": "http://github.com/vanils/loose-memoize.git"
  },
  "scripts": {
    "build": "rollup -c ./chore/rollup.dist.js",
    "compare": "yarn prepare && node ./chore/compare/runTests.js",
    "test": "yarn test:node",
    "test:node": "yarn prepare && nyc --reporter=lcov mocha",
    "test:browser": "yarn prepare && yarn build:test && http-server ./dist-test -o",
    "test:v8": "yarn prepare && node --trace-deopt --trace-opt chore/v8.report.js",
    "build:test": "yarn build:test-rollup --silent && yarn build:test-webpack --silent",
    "build:test-rollup": "rollup -c ./chore/rollup.test.js",
    "build:test-webpack": "webpack --config ./chore/webpack.test.js",
    "lint": "eslint \"**/*.js\"",
    "prettier": "prettier \"**/*{.js,.json}\"",
    "clean": "node ./chore/clean.js",
    "prepare": "yarn clean && yarn build --silent"
  },
  "keywords": [
    "memoize",
    "memoizer",
    "memoization",
    "memo",
    "memcached",
    "memory",
    "cache"
  ],
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-memoize": "^2.5.1",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "inquirer": "^6.2.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "memoizee": "^0.4.14",
    "mocha": "^5.2.0",
    "nano-memoize": "^1.0.0",
    "nyc": "^13.1.0",
    "ora": "^3.0.0",
    "p-memoize": "^2.1.0",
    "prettier": "^1.15.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.1",
    "sinon": "^7.2.2",
    "style-loader": "^0.23.1",
    "table": "^5.2.1",
    "underscore": "^1.9.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "word-wrap": "^1.2.3",
    "yargs": "^12.0.5"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "mocha": true,
      "node": true,
      "es6": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "nyc": {
    "temp-dir": "node_modules/.cache/nyc_output",
    "reporter": [
      "lcov",
      "html"
    ]
  }
}
